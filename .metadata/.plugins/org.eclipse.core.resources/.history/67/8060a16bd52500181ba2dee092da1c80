package mainwin;
import java.util.*;
import javax.swing.*;
import java.awt.*;
import function.*;
import javax.swing.GroupLayout.Alignment;
import javax.swing.LayoutStyle.ComponentPlacement;

public class window extends JFrame{
	int height=700;
	int width=800;
	int x=100;
	int y=50;
	private JTextField textField;
	private JTextField textField_1;
	public window() {
		
		setBounds(x, y, width, height);
		setDefaultCloseOperation(WindowConstants.EXIT_ON_CLOSE);
		JLabel label = new JLabel("选择导入文件：");
		label.setBounds(66, 601, 91, 16);
		
		textField = new JTextField();
		textField.setBounds(169, 596, 431, 26);
		textField.setColumns(10);
		
		JLabel label_1 = new JLabel("选择导出文件：");
		label_1.setBounds(66, 639, 91, 16);
		
		textField_1 = new JTextField();
		textField_1.setBounds(169, 634, 431, 26);
		textField_1.setColumns(10);
		getContentPane().setLayout(null);
		getContentPane().add(label_1);
		getContentPane().add(textField_1);
		getContentPane().add(label);
		getContentPane().add(textField);
		
		JButton button = new JButton("选择文件");
		button.setBounds(633, 596, 117, 29);
		getContentPane().add(button);
		
		JButton button_1 = new JButton("选择文件");
		button_1.setBounds(633, 634, 117, 29);
		getContentPane().add(button_1);
		
		JTextArea textArea = new JTextArea();
		textArea.setBounds(66, 120, 254, 449);
		getContentPane().add(textArea);
		
	}

	public static void main(String[] args) {
		int height=700;
		int width=800;
		int x=100;
		int y=50;
		// TODO Auto-generated method stub
		window win = new window();
		win.setTitle("加密软件");

		win.show();
	}
}
